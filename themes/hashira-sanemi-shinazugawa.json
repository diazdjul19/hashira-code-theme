  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#778899"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#A0522D"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "keyword.operator.arithmetic"],
      "settings": {
        "foreground": "#2F4F4F"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#4682B4"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#2E8B57"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#3CB371"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Parameter",
      "scope": ["variable.parameter", "meta.function.parameter"],
      "settings": {
        "foreground": "#556B2F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Property",
      "scope": ["entity.name.property", "support.type.property-name"],
      "settings": {
        "foreground": "#A52A2A"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#2F4F4F"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name", "support.class.attribute"],
      "settings": {
        "foreground": "#2E8B57",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Library Constant",
      "scope": ["support.constant", "constant.language"],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Regular Expressions and Escape Characters",
      "scope": ["string.regexp", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#008B8B"
      }
    },
    {
      "name": "HTML - TEXT",
      "scope": ["text.html.derivative"],
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Typescript Class Types",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#4682B4"
      }
    }
  ]
}
