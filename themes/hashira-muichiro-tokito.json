  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#64748b"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "constant.other.symbol",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#86efac"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#d8b4fe"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#d1d5db"
      }
    },
    {
      "name": "Language variable",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#f97316"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#f97316"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#c0c5ce"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "storage.type"],
      "settings": {
        "foreground": "#a5f3fc"
      }
    },
    {
      "name": "Function name",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Function argument",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#d8b4fe"
      }
    },
    {
      "name": "Class name",
      "scope": ["entity.name.class"],
      "settings": {
        "foreground": "#f97316"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml"],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Attribute name",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Library function",
      "scope": ["support.function"],
      "settings": {
        "foreground": "#fde047"
      }
    },
    {
      "name": "Library constant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Library class/type",
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#f97316"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Markup heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#86efac"
      }
    },
    {
      "name": "Markup links",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#7dd3fc"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.raw"],
      "settings": {
        "background": "#c0c5ce05"
      }
    },
    {
      "name": "Markup Code Inline",
      "scope": ["markup.raw.inline"],
      "settings": {
        "background": "#c0c5ce0f"
      }
    },
    {
      "name": "Markdown Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#64748b"
      }
    },
    {
      "name": "Markup Blockquote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#7dd3fc",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup List Bullet",
      "scope": ["markup.list punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#fde047"
      }
    },
    {
      "name": "Markup added",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#86efac"
      }
    },
    {
      "name": "Markup modified",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#3b82f6"
      }
    },
    {
      "name": "Markup removed",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#f472b6"
      }
    }
  ]
}
